{
  "ConnectionStrings": {
    "MongoDb": "mongodb://localhost:27017"
  },
  "LogSettings": {
    "ApplicationName": "LogLib",
    "Environment": "Development",
    "DatabaseName": "LogLibLogs",
    "ExpireAfterDays": 30,
    "ResetDatabaseOnStartup": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
